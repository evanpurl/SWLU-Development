$Sectors = [PLAYERSHIP]-> find all sectors within 9999 jumps: Only known sectors=[FALSE]
$SectorCount = size of array $Sectors
$SectorOwnerData = array alloc: size=0
$SectorOwnerData.Sectors = array alloc: size=0
$SectorOwnerData.Owners = array alloc: size=0


* 0 Array of sectors
* 1 Array of Owners, matching sector indexes

while $SectorCount
 dec $SectorCount
 $SectorCurrent = $Sectors[$SectorCount]
 $Owner = $SectorCurrent-> get owner race
 append $SectorCurrent to array $SectorOwnerData.Sectors
 append $Owner to array $SectorOwnerData.Owners
end

append $SectorOwnerData.Sectors to array $SectorOwnerData
append $SectorOwnerData.Owners to array $SectorOwnerData

set global variable: name=$Key.SectorOwnerData value=$SectorOwnerData

return null



