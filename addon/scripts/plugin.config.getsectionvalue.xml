<line linenr="020" indent="&#160;"><var>$a&#160;=&#160;</var><var>$aOptions</var><text>[</text><var>$count</var><text>]</text></line>
<line linenr="021" indent="&#160;"><var>$id&#160;=&#160;</var><var>$aOptions</var><text>[</text><var>$count</var><text>][</text><var>0</var><text>]</text></line>
<line linenr="022" indent="&#160;"><var>if&#160;</var><var>$id</var><text>&#160;</text><var>==</var><text>&#160;</text><var>$a.id</var></line>
<line linenr="023" indent="&#160;&#160;"><var>$value&#160;=&#160;</var><var>$aOptions</var><text>[</text><var>$count</var><text>][</text><var>4</var><text>]</text></line>
<line linenr="024" indent="&#160;&#160;"><text>return&#160;</text><var>$value</var></line>
